const ZEBRA_LOGIN_URL = "{{ kpi_frontend_login_url }}";

export function getAuthUrl(url) {
  return ZEBRA_LOGIN_URL + "?return_url=" + url;
}

export function getZebraLoginUrl() {
  return ZEBRA_LOGIN_URL;
}

